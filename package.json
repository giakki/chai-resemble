{
    "name": "chai-resemble",
    "version": "2.0.1",
    "author": "Giacomo Martino",
    "description": "Chai helper for visually comparing HTML pages",
    "license": "MIT",
    "homepage": "https://github.com/giakki/chai-resemble",
    "main": "src/index.js",
    "scripts": {
        "cover": "nyc npm run mocha",
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "lint": "eslint .",
        "mocha": "mocha --reporter spec --timeout 25000",
        "test": "npm run lint && npm run mocha",
        "travis": "npm run lint && npm run cover"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/giakki/chai-resemble.git"
    },
    "bugs": {
        "url": "https://github.com/giakki/chai-resemble/issues"
    },
    "keywords": [
        "chai",
        "css",
        "gm",
        "regression",
        "resemble",
        "testing"
    ],
    "dependencies": {
        "looks-same": "~7.2.3",
        "make-dir": "^3.1.0",
        "puppeteer": "^5.3.1",
        "tmp-promise": "^3.0.2"
    },
    "devDependencies": {
        "chai": "~4.2.0",
        "coveralls": "^3.1.0",
        "eslint": "~7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "mocha": "~8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.2"
    },
    "files": [
        "src/**/*",
        "LICENSE"
    ],
    "prettier": {
        "endOfLine": "lf",
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    }
}
